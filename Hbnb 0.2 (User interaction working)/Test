POST USER ✅
-----------
curl -X POST http://localhost:5000/users \
-H "Content-Type: application/json" \
-d '{
  "email": "epicder11@example.com",
  "password": "iloveflask123",
  "first_name": "Elian",
  "last_name": "Gonzalez"
}'

GET ALL USERS ✅
-------------
curl -X GET http://localhost:5000/users

GET ID USER ✅
-------------
curl -X GET http://localhost:5000/users/1f1b8adf-88ed-4bbc-bbb8-2b91d76d4640


ACTUALIZAR USER POR ID ✅
-------------
curl -X PUT http://localhost:5000/users/1f1b8adf-88ed-4bbc-bbb8-2b91d76d4640 \
-H "Content-Type: application/json" \
-d '{
  "email": "EmailActualizado@example.com",
  "password": "ilovePUTmethod123",
  "first_name": "Never gonna",
  "last_name": "Gonzalez"
}'


DELETE USER POR ID ✅
--------------
curl -X DELETE http://localhost:5000/users/1f1b8adf-88ed-4bbc-bbb8-2b91d76d4640


CREATE PLACE ❔
--------------

curl -X POST http://localhost:5000/places \
     -H "Content-Type: application/json" \
     -d '{
           "name": "Lugar_increible",
           "host_id": "host_id_del_place",
           "description": "Este lugar es hermoso",
           "rooms": 2,
           "bathrooms": 1,
           "max_guests": 4,
           "price_per_night": 100.0,
           "latitude": 40.7128,
           "longitude": -74.0060,
           "city_id": "city_id_del_place",
           "amenities_ids": ["amenity_id_1", "amenity_id_2"]
         }'

CREATE REVIEW ❔
--------------

curl -X POST http://localhost:5000/reviews \
     -H "Content-Type: application/json" \
     -d '{
           "rating": 5,
           "user_id": "user_id_del_review",
           "place_id": "place_id_del_review",
           "comment": "Este lugar es increíble!"
         }'

CREATE AMENITY ❔
--------------

curl -X POST http://tuservidor.com/amenities \
     -H "Content-Type: application/json" \
     -d '{"id": 123, "name": "Wifi"}'